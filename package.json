{
  "name": "fennec-animation",
  "version": "0.1.0",
  "private": true,
  "description": "Config-driven, audio-synced canvas animation. Static export (GitHub Pages friendly).",
  "license": "MIT",
  "author": "Michelle <contact@michellef.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/michellevit/Fennec-Animation.git"
  },
  "homepage": "https://fennec.michellef.dev",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "powershell -ExecutionPolicy Bypass -File .\\scripts\\clean.ps1",
    "build": "npm run clean && next build",
    "build-mac": "next build",
    "deploy": "npm run build && gh-pages -d out --dotfiles",
    "deploy-mac": "next build && gh-pages -d out --dotfiles",
    "dev": "next dev --turbopack -p 3000",
    "start": "next start",
    "lint": "next lint",
    "serve-out": "npx serve out -l 5000",
    "serve-out:lan": "npx serve out -l tcp://0.0.0.0:5000",
    "preview": "npm-run-all build serve-out",
    "preview-mac": "npm-run-all build-mac serve-out"
  },
  "keywords": [
    "nextjs",
    "react",
    "canvas",
    "pixel-art",
    "animation",
    "audio",
    "static-export",
    "gh-pages"
  ],
  "dependencies": {
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "gh-pages": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5"
  }
}